//seraching in js

//function to find element

function findElement(arr, key) {
  for (let i = 0; i < arr.length; i++) {
    if (arr[i] == key) {
      return i;
    }
  }
  return -1;
}

//implement
let arr = [2, 4, 5, 1, 6, 2];
let key = 6;

let result = findElement(arr, key);

if (result == -1) {
  console.log(`Element not found`);
} else {
  console.log(`The position of ${key} is ${result}`);
}
